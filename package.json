{
  "name": "ChatApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "__your__scripts__": "",
    "apk": "cd android &&./gradlew clean && ./gradlew assembleRelease && cd ..",
    "app-apk": "cd android &&./gradlew app:assembleRelease && cd ..",
    "variant": "react-native run-android --variant=release",
    "rm-apk": "adb shell pm uninstall --user 0 com.chatapp",
    "clear-cache": "adb shell pm clear com.chatapp",
    "aab": "cd android &&./gradlew clean && ./gradlew bundleRelease && cd ..",
    "rm": "rm -rf ./node_modules",
    "rm-pod": "cd ios && rm Podfile.lock",
    "pod": "npx pod-install",
    "link": "npx react-native link",
    "asset": " npx react-native-asset",
    "upgrade": "npx react-native upgrade"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.71.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
